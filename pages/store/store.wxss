/* pages/store/store.wxss */
.title {
  width: 750rpx;
  height: 100rpx;
  line-height: 100rpx;
  vertical-align: middle;
  border-bottom: 2rpx solid #F2F2F2;
}

.title image {
  width: 24rpx;
  height: 14rpx;
}

.left {
  float: left;
  margin-left: 28rpx;
  font-size: 30rpx;
  color: #323232;
}

.right {
  float: right;
  margin-right: 22rpx;
  font-size: 30rpx;
  color: #666;
}

.store-container {
  position: relative;
  width: 750rpx;
  height: 180rpx;
  border-bottom: 2rpx solid #F2F2F2;
}

.store-title {
  position: absolute;
  top: 37rpx;
  left: 27rpx;
  width: 500rpx;
  height: 40rpx;
  line-height: 40rpx;
  vertical-align: middle;
}

.self-support {
  width: 116rpx;
  height: 32rpx;
  line-height: 32rpx;
  text-align: center;
  vertical-align: middle;
  color: #FFF;
  font-size: 22rpx;
  display: inline-block;
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
}

.store-name {
  font-size: 28rpx;
  color: #333;
  height: 40rpx;
  line-height: 40rpx;
  vertical-align: middle;
  display: inline-block;
  margin-left: 15rpx;
}

.store-position {
  font-size: 24rpx;
  color: #666;
  position: absolute;
  left: 27rpx;
  top: 90rpx;
  width: 568rpx;
  height: 60rpx;
}

.store-distance {
  width: 120rpx;
  height: 180rpx;
  font-size: 24rpx;
  color: #666;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  position: absolute;
  top: 0;
  right: 10rpx;
}

.store-distance image {
  width: 23rpx;
  height: 31rpx;
}